{
	"name": "studiometa/wp",
	"description": "Manage and autoload your WordPress assets based on a YAML configuration file.",
  "version": "1.0.0",
	"license": "MIT",
	"require": {
		"symfony/yaml": "^5.0",
		"php": ">=7.0"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.4",
		"wp-coding-standards/wpcs": "^2.1",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.5.0"
	},
  "autoload": {
    "psr-4": {
      "Studiometa\\WP\\": "src/"
    }
  }
}
